//pacijentu su u poslednjih 6 meseci barem 6 puta prepisani anelgetici od strane 3 ili vishe 
//razlicita lekara
rule "spisak mogucih zavisnika"
salience 300
agenda-group "moguciZavisnici"
	when 
		$ta:JTextArea(  )
		$p: Pacient(  $ter: terapije )
		
		
		
		$l : Long(intValue >5) from accumulate(
			$pt:PrepisanaTerapija($pre : prepisaniLekovi  ,
			datumPrepisaneTerapiej.isAfter(LocalDate.now().minusMonths(6))
			 )	from $ter  and
			 $lista:List(size >0) from collect ($lek:Lek(tipLeka.toString  =="ANELGETIK") from $pre)
			 ,count($pt)
		 )
		 
		 $setLekari : Set( size>2 ) from accumulate( 
			 $ptL :PrepisanaTerapija( $lekar:lerarIliAdmin ,
			 datumPrepisaneTerapiej.isAfter(LocalDate.now().minusMonths(6))) from $ter and
			 o1:Osoba(  ) from $lekar,
			 collectSet(o1)
		 )
	

		// $number :Long(intValue >0) from accumulate(
		//	 $o1:Osoba(  ) from $listLekari and
		//	 $o2:Osoba(this != $o1) from $listLekari and
		//	 $o3:Osoba(this != $o1 , this != $o2) from $listLekari ,
		//	 count($o3)
		// )
		 
		
	then
		System.out.println("pokrentut zavisnici " + $p);
		$ta.setText($ta.getText()+ "	"+$p+"\n");
end





